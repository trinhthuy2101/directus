{
	"name": "directus-extension-hook",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^9.24.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "9.24.0",
		"@types/node": "^20.2.3",
		"@types/nodemailer": "^6.4.8",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"@directus/types": "^10.0.1",
		"axios": "^1.4.0",
		"nodemailer": "^6.9.2",
		"sharp": "^0.32.1"
	}
}
